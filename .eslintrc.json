{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    // "extends": [
    //     "eslint:recommended",
    //     "plugin:vue/essential",
    //     "plugin:@typescript-eslint/eslint-recommended"
    // ],
    "extends": [
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    // "parserOptions": {
    //     "ecmaVersion": 2018,
    //     "parser": "@typescript-eslint/parser",
    //     "sourceType": "module"
    // },
    // "plugins": [
    //     "vue",
    //     "@typescript-eslint"
    // ],
    "rules": {
        "indent": [
            2,
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}
